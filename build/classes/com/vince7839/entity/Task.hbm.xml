<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.vince7839.entity">
    <class name="Task" table="task">
         <id name="id" column="id">  
            <generator class="increment" />
        </id>  
        <property name="projectId" type="int" column="project_id" />
        <property name="status" type="" column="status" />
        <property name="startDate" type="date" column="start_date" />
        <property name="expectDate" type="date" column="expect_date" />
        <property name="endDate" type="date" column="end_date" />
        <property name="summary" type="string" column="summary" />
        <property name="softwareType" type="int" column="software_type" />
        <property name="bugId" type="int" column="bug_id" />
        <property name="tag" type="int" column="tag" />
    </class>
</hibernate-mapping>